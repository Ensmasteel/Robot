        </param>
        <param>
          <type>uint8_t</type>
          <declname>pinDir</declname>
        </param>
        <param>
          <type>uint8_t</type>
          <declname>pinStep</declname>
        </param>
        <param>
          <type>uint8_t</type>
          <declname>pinSleep</declname>
        </param>
